<p id="notice"><%= notice %></p>

<p>
  <strong>Name:</strong>
  <%= @ticket.name %>
</p>

<p>
  <strong>Address:</strong>
  <%= @ticket.address %>
</p>

<p>
  <strong>Price:</strong>
  <%= @ticket.price %>
</p>

<p>
  <strong>Phone:</strong>
  <%= @ticket.phone %>
</p>

<p>
  <strong>Email address:</strong>
  <%= @ticket.email_address %>
</p>

<button type="button" class="btn btn-outline-danger btn-round">
<%= link_to 'Edytuj', edit_ticket_path(@ticket) %> |
</button>
<button type="button" class="btn btn-outline-danger btn-round">
<%= link_to 'Wróć', tickets_path %>
</button>